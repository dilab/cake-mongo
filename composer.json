{
    "name": "imdad/cake-mongo",
    "description": "A MongoDB datasource and data mapper for CakePHP 3.3",
    "type": "cakephp-plugin",
    "require": {
        "cakephp/cakephp": "~3.3",
        "mongodb/mongodb": "^1.1"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "~2.1",
        "psr/log": "~1.0",
        "phpunit/phpunit": "~4.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "SK Imdad",
            "email": "imdad.sk@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dilab\\CakeMongo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dilab\\CakeMongo\\Test\\": "tests",
            "TestPlugin\\": "tests/testapp/Plugin/TestPlugin/src",
            "TestPluginTwo\\": "tests/testapp/Plugin/TestPluginTwo/src"
        }
    }
}
